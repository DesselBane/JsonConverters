<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>JsonConverters</PackageId>
        <Version>1.0.0</Version>
        <Authors>Dark Phantom</Authors>
        <Company>DarkSolutions</Company>
        <Title>JsonConverters</Title>
        <Description>A Package with Converters for Newtonsoft.Json.</Description>
        <PackageLicenseUrl>https://github.com/DesselBane/JsonConverters/blob/master/Licence.txt</PackageLicenseUrl>
        <PackageProjectUrl>https://github.com/DesselBane/JsonConverters.git</PackageProjectUrl>
        <RepositoryUrl>https://github.com/DesselBane/JsonConverters.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Converter;Newtonsoft.Json</PackageTags>
        <IncludeSource>True</IncludeSource>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Newtonsoft.Json" Version="11.0.2"/>
    </ItemGroup>

</Project>
